{
    "name": "wamesk/laravel-nova-currency",
    "description": "Laravel Nova 4 Currency field.",
	"version": "4.0.0",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "currency",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "calebporzio/sushi": "^v2.5.2",
        "interaction-design-foundation/nova-html-card": "^3.0",
        "laravel/nova": "^4.0",
        "moneyphp/money": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaCurrency\\": "src/",
            "Wame\\LaravelNovaCurrency\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaCurrency\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
