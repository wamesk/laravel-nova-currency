{
    "name": "wamesk/laravel-nova-currency",
    "description": "Laravel Nova 4 Currency field.",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "currency",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "^4.0",
        "interaction-design-foundation/nova-html-card": "^3.0",
        "wamesk/laravel-commands": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaCurrency\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaCurrency\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
